parameters:
  phpVersion: 80300 # PHP 8.3
  bootstrapFiles:
    - tests/bootstrap.php
  paths:
    - src
    - tests
  level: 9
  treatPhpDocTypesAsCertain: false
  ignoreErrors:
    - identifier: missingType.iterableValue
    - identifier: missingType.generics
  excludePaths:
    - tests/comparisons/*
